{
  "name": "storycap-delay-repro",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook::server": "http-server ./dist/storybook -p 4243 --silent --gzip",
    "storybook::storycap": "storycap http://localhost:4243 --outDir \"./regression-test/actual\" --disableWaitAssets true",
    "storybook::capture": "run-p --print-label --race storybook::server storybook::storycap",
    "storybook:start": "npm run docs:json && start-storybook -p 4242",
    "storybook:build": "npm run docs:json && build-storybook -o ./dist/storybook",
    "storybook:test": "npm run storybook:build && npm run storybook::capture"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.0",
    "@babel/core": "^7.17.9",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/angular": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/testing-library": "0.0.11",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.5",
    "http-server": "^14.1.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-run-all": "^4.1.5",
    "storybook-ng-source-addon": "0.0.18",
    "storycap": "^3.1.7",
    "typescript": "~4.3.5"
  }
}
